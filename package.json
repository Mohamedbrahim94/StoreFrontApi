{
    "name": "storefront",
    "version": "1.0.0",
    "description": "storeFront backend",
    "main": "index.js",
    "scripts": {
        "build": "npx tsc",
        "start-dev": "node src/server.ts",
        "start": "nodemon --exec npx ts-node src/server.ts",
        "watch": "tsc-watch esModuleIntrerop  src/server.ts --outDir ./build  --onSuccess 'node ./build/server.jsy'",
        "prettier": "prettier --write src/**/*.{ts,js,} ",
        "lint": "eslint . --ext .ts",
        "lint-fix": "eslint . --fix  ",
        "migrate": "db-migrate --env test up && db-migrate up ",
        "migrate-up": "npx db-migrate up",
        "migrate-down": "npx db-migrate down",
        "jasmine": "jasmine",
        "test": "npm run build&&export ENV=test||set ENV=test&& npm run migrate &&npm run jasmine && npx db-migrate reset"
    },
    "author": "Mohamed Ibrahim",
    "license": "ISC",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/jasmine": "^3.10.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.16",
        "@types/pg": "^8.6.4",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "bcrypt": "^5.0.1",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^4.0.2",
        "jasmine-spec-builder": "^1.0.9",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts-console-reporter": "^3.1.1",
        "jsonwebtoken": "^8.5.1",
        "prettier": "^2.5.1",
        "supertest": "^6.2.2",
        "ts-node": "^10.5.0",
        "tsc-watch": "^4.6.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "nodemon": "^2.0.15",
        "pg": "^8.7.3"
    }
}